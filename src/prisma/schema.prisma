// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int        @id @default(autoincrement())
  name       String       
  /// 0 - 100 value 0, banned 45, unverified user (no email verification) 50, user 100, admin 
  privilige  Int
  email      String     @unique
  password   String
  createdAt  DateTime   @default(now())
}
